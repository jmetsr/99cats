<!DOCTYPE html>
<html>

<% if !current_user.nil? %>
<ul>
	<li> Logged in as: <%= current_user.username %> </li>
	<li> <%= button_to "Logout", session_url(session), method: :delete %>
</ul>

<% else %>
	<a href="<%= new_session_url %>"> Log In </a>

<% end %>
<head>
  <title>RentalCats</title>
</head>
<body>

<%= yield %>

</body>

</html>