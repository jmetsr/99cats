<form action="<%= cat_rental_requests_url %>" method="POST">
<h1> File A Cat Rental Request Form 1040 </h1>
<label for="cat_id">Cat to be Rented</label>
	<select name="@cat_rental_request[cat_id]" id="cat_id">
		<% Cat.all.each do |cat| %>
			<option value=<%= cat.id %>><%= cat.name %></option>
		<% end %>
	</select>
	<br>
	<label for="rental_start_date">Rental Start Date</label>
	<input type="date" name="@cat_rental_request[start_date]" id="rental_start_date" value="<%= @cat_rental_request.start_date %>">
	<br>
	<label for="rental_end_date">Rental End Date</label>
	<input type="date" name="@cat_rental_request[end_date]" id="rental_end_date" value="<%= @cat_rental_request.end_date %>">
	<br>
	<input type="submit" value="File your request!">
</form>
